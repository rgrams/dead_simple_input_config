[project]
title = Dead Simple Input Config
version = 0.1
dependencies = https://github.com/rgrams/rendercam/archive/master.zip https://github.com/rgrams/another_defold_timer/archive/master.zip

[bootstrap]
main_collection = /main/main.collectionc
render = /rendercam/rendercam.renderc

[physics]
scale = 0.02

[script]
shared_state = 1

[input]
game_binding = /simple_input_config/all_inputs.input_bindingc
use_accelerometer = 0

[library]
include_dirs = simple_input_config

